{
    "name": "Open osu! from Chrome Extension",
    "version": "1.0.0",
    "description": "Open osu! from the Extension",
    "manifest_version": 3,
    "author": "GGchung",
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "permissions": [
      "storage"
    ],
    "browser_action": {
      "default_icon": {
         "16": "icons/16.png",
         "48": "icons/48.png",
         "128": "icons/128.png"
      },
      "default_popup": "index.html",
      "default_title": "Open on install"
   }
   "action": {
    "default_popup": "manual.html",
    "default_title": "Manually open osu!"
  }
}

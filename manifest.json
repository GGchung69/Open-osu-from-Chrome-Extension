{
    "name": "Open osu! from Chrome Extension",
    "version": "1.0.0",
    "description": "Open osu! from the Extension",
    "manifest_version": 3,
    "author": "GGchung",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
      "storage"
    ],
    "browser_action": {
      "default_icon": {
         "16": "icons/icon16.png",
         "48": "icons/icon48.png",
         "128": "icons/icon128.png"
      },
      "default_popup": "index.html",
      "default_title": "Open"
   }
}
